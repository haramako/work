#!/usr/bin/env clisp

(require 'fc)

(progn
(fc-func fib (x)
         (if (eq x 0)
             1
           (+ (fib (- x 1)) (fib (- x 2)))))
         
(fc-func add (a b)
         (if a 1 2)
         (fc-loop (+ a b))
         ;(case a (1 1) (2 2))
         )
         ;(+ a (+ b 1)))
)
(fc-func sub (a b)
          (- a b))

(fc-func main () )

(print fc-func-alist)

(maphash #'(lambda (name func)
             (fcasm-compile-func name (second func) (third func)))
         fc-func-alist)
(fcasm-output "out.asm")

